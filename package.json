    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/cypress": "^0.1.6",
    "@types/node": "^22.10.2",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^8.2.0",
    "cypress": "^13.16.1",
    "cypress-vite": "^1.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "ts-node": "^10.9.2",
    "vite": "^5.4.11"
  },
  "dependencies": {
    "vitest": "^2.0.3"
  }
